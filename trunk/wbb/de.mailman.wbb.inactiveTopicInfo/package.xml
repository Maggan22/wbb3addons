<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE language SYSTEM "http://www.woltlab.com/DTDs/package.dtd">
<package name="de.mailman.wbb.inactiveTopicInfo">
	<packageinformation>
		<packagename>Inactive Thread/Post Info</packagename>
		<packagename language="fr">Information sur les discussion/messages inactifs</packagename>
		<packagename language="da">Viser meddelelse om hvorvidt en tråd/et indlæg er lukket, deaktiveret eller slettet.</packagename>
		<packagedescription language="de">Zeigt eine Hinweismeldung an, wenn ein Thread/Post bereits geschlossen, deaktiviert oder gelöscht ist.</packagedescription>
		<version>1.0.1</version>
		<date>2008-03-04</date>
		<plugin>com.woltlab.wbb</plugin>
		<isunique>1</isunique>
	</packageinformation>
	
	<authorinformation>
		<author>MailMan</author>
		<authorurl>http://wbb3addons.ump2002.net</authorurl>
	</authorinformation>
 
	<requiredpackages>
		<requiredpackage minversion="3.0.4 pl 1">com.woltlab.wbb</requiredpackage>
	</requiredpackages>

	<instructions type="install">
	    <files>files.tar</files>
	    <options>options.xml</options>
	    <eventlistener>eventlistener.xml</eventlistener>
		<languages languagecode="de">de.xml</languages>
		<languages languagecode="da">da.xml</languages>
		<languages languagecode="de-informal">de-informal.xml</languages>
		<languages languagecode="en">en.xml</languages>
		<languages languagecode="fr">fr.xml</languages>
	</instructions>
	<instructions type="update" fromversion="1.0.0">
	    <files>files.tar</files>
	    <options>options.xml</options>
		<languages languagecode="de">de.xml</languages>
		<languages languagecode="da">da.xml</languages>
		<languages languagecode="de-informal">de-informal.xml</languages>
		<languages languagecode="en">en.xml</languages>
		<languages languagecode="fr">fr.xml</languages>
	</instructions>
</package>
