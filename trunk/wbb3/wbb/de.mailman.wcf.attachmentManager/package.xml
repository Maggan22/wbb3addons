<package name="de.mailman.wcf.attachmentManager">
	<packageinformation>
		<packagename>AttachmentManager</packagename>
		<packagedescription>Attachment Verwaltung.</packagedescription>
		<isUnique>1</isUnique>
		<version>1.0.0</version>
		<date>2008-02-07</date>
		<standalone>0</standalone>
		<plugin>com.woltlab.wcf</plugin>
	</packageinformation>

	<authorinformation>
		<author>MailMan</author>
