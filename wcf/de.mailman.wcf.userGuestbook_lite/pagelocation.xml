<?xml version="1.0"?>
<!DOCTYPE import SYSTEM "DTD/import.dtd">
<data>
	<import>
		<pagelocation name="wcf.usersOnline.location.userGuestbookList">
			<pattern><![CDATA[index\.php\?page=UserGuestbookList.*]]></pattern>
			<classpath>lib/data/page/location/UserGuestbookLocation.class.php</classpath>
		</pagelocation>
		<pagelocation name="wcf.usersOnline.location.userGuestbookView">
			<pattern><![CDATA[index\.php\?page=UserGuestbook&.*userID=(\d+)]]></pattern>
			<classpath>lib/data/page/location/UserGuestbookLocation.class.php</classpath>
		</pagelocation>
		<pagelocation name="wcf.usersOnline.location.userGuestbookAddEntry">
			<pattern><![CDATA[index\.php\?form=UserGuestbookNew&.*userID=(\d+)]]></pattern>
			<classpath>lib/data/page/location/UserGuestbookLocation.class.php</classpath>
		</pagelocation>
	</import>
</data>
