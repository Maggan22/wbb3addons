<package name="de.mailman.wcf.userWantedPoster.lite">
	<packageinformation>
		<packagename>Benutzer-Steckbrief [WBB Lite 2]</packagename>
		<packagedescription>Eine &quot;eigene&quot; Seite für den Benutzer. [WBB Lite 2]</packagedescription>
		<packagename language="en">User Wanted Poster  [WBB Lite 2]</packagename>
		<packagedescription language="en">A special page for the user. [WBB Lite 2]</packagedescription>
		<isUnique>1</isUnique>
		<version>1.0.10</version>
		<date>2008-09-05</date>
		<!-- <standalone>0</standalone> -->
		<plugin>com.woltlab.wcf</plugin>
	</packageinformation>

	<authorinformation>
		<author>MailMan</author>
		<authorurl>http://wbb3addons.ump2002.net</authorurl>
	</authorinformation>

	<requiredpackages>
        <requiredPackage minversion="1.0.5">com.woltlab.wcf</requiredPackage>
        <requiredPackage minversion="1.0.6" file="required/com.woltlab.community.acp.wysiwyg.tar">com.woltlab.community.acp.wysiwyg</requiredPackage>
	</requiredpackages>

	<instructions type="install">
		<sql>install.sql</sql>
		<files>files.tar</files>
		<templates>templates.tar</templates>
		<acptemplates>acptemplates.tar</acptemplates>
		<options>options.xml</options>
		<cronjobs>cronjobs.xml</cronjobs>
		<groupoptions>groupoptions.xml</groupoptions>
        <usercpmenu>usercpmenu.xml</usercpmenu>
        <userprofilemenu>userprofilemenu.xml</userprofilemenu>
		<acpmenu>acpmenu.xml</acpmenu>
        <pagelocation>pagelocation.xml</pagelocation>
        <headermenu>headermenu.xml</headermenu>
        <eventlistener>eventlistener.xml</eventlistener>
		<languages languagecode="de">de.xml</languages>
        <languages languagecode="de-informal">de-informal.xml</languages>
        <languages languagecode="en">en.xml</languages>
	</instructions>

