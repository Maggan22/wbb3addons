<?xml version="1.0"?>
<!DOCTYPE data SYSTEM "http://www.woltlab.com/DTD/options.dtd">
<data>
    <import>
			<functions>
				
				<function name="wbb.boardPermissions">
					<classpath>lib/acp/admintools/function/BoardPermissionsAdminToolsFunction.class.php</classpath>
				</function>
				
			</functions>
	    
	    <categories>
			
			<category name="wbb">
			</category>
			
			<category name="wbb.boardPermissions">
				<function>wbb.boardPermissions</function>
				<parent>wbb</parent>
			</category>			
			
	    </categories>
	    
	    <options>
	    	<option name="wbb.boardPermissions.sourceBoard">
    			<categoryname>wbb.boardPermissions</categoryname>
    			<optiontype>boardsingleselect</optiontype>    			
    			<showorder>1</showorder>
    		</option>
    		
    		<option name="wbb.boardPermissions.targetBoards">
    			<categoryname>wbb.boardPermissions</categoryname>
    			<optiontype>boardmultiselect</optiontype>    			
    			<showorder>2</showorder>
    		</option>
    		
    		<option name="wbb.boardPermissions.copyUsers">
    			<categoryname>wbb.boardPermissions</categoryname>
    			<optiontype>boolean</optiontype>
    			<defaultvalue>0</defaultvalue>
    			<showorder>3</showorder>
    		</option>
    		
    		<option name="wbb.boardPermissions.copyUsergroups">
    			<categoryname>wbb.boardPermissions</categoryname>
    			<optiontype>boolean</optiontype>
    			<defaultvalue>0</defaultvalue>
    			<showorder>4</showorder>
    		</option>
    		
    		<option name="wbb.boardPermissions.copyModerators">
    			<categoryname>wbb.boardPermissions</categoryname>
    			<optiontype>boolean</optiontype>
    			<defaultvalue>0</defaultvalue>
    			<showorder>5</showorder>
    		</option>
    		    		
		</options>
    </import>
</data>